                      var itemTitle     = dataPreviousPage.items[i].htmlTitle;
                      var itemSiteName  = dataPreviousPage.items[i].pagemap.metatags[0]['og:site_name'];
                      var itemLink      = dataPreviousPage.items[i].link.replace(/\?.*/g,'');
